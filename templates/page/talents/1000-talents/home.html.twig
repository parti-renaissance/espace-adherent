{% extends 'page/talents/layout.html.twig' %}

{% block page_title page.title %}
{% block canonical_url url('page_1000_talents') %}
{% block meta_description page.title ~ ' : ' ~ page.description %}
{% block meta_twitter_description page.twitterDescription ?: page.description %}
{% set talents_header_image = 'action-talents/home.jpg' %}

{% block talents_header_content %}
    <p>{{ page.description }}</p>
    <br/><br/><br/>
{% endblock %}

{% block talents_content %}
    <div class="space--20-0">
        {{ page.content|markdown_to_html }}
    </div>
{% endblock %}
