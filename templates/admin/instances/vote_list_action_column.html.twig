{% set filters = {designation: {value: object.election.designation.id}} %}

{% if object.election.designationType == 'copol' %}
    {% set filters = filters|merge({territorialCouncil: {value: object.election.electionEntity.territorialCouncil.id}}) %}
{% else %}
    {% set filters = filters|merge({committee: {value: object.election.electionEntity.committee.id}}) %}
{% endif %}

<a href="{{ path('admin_app_algolia_algoliacandidature_list', {filter: filters}) }}" class="btn btn-sm btn-default" title="Candidatures">
    <i class="fa fa-users"></i>
</a>
