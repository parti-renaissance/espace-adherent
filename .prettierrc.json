{
    "plugins": ["prettier-plugin-gherkin"],
    "tabWidth": 4,
    "printWidth": 180,
    "semi": true,
    "singleQuote": true,
    "trailingComma": "es5",
    "useTabs": false,
    "arrowParens": "always",
    "bracketSpacing": true,
    "endOfLine": "auto",
    "bracketSameLine": false,
    "proseWrap": "preserve"
}
